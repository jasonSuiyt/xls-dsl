{
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm start --port 1420",
    "devPath": "http://localhost:1420",
    "distDir": "../dist/xls-dsl",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "xls-dsl",
    "version": "0.0.0"
  },
  "plugins": {
    "shell": {
      "open": true
    }
  },
  "tauri": {
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "jasonsui",
      "targets": "all"
    },
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 750,
        "minHeight": 380,
        "minWidth": 750,
        "resizable": true,
        "title": "",
        "titleBarStyle": "Overlay",
        "transparent": true,
        "width": 1280
      }
    ]
  }
}